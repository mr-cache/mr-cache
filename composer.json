{
    "name": "mrcache/mrcache",
    "description": "A smart Redis caching layer for PHP frameworks Eloquent ORM.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "MrCache\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Ajmal Alkhaledi",
            "email": "ajmalalkhaledi@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "illuminate/contracts": "^12.0",
        "illuminate/database": "^12.0",
        "illuminate/support": "^12.0",
        "illuminate/console": "^12.0",
        "illuminate/events": "^12.0",
        "predis/predis": "^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload-dev": {
        "psr-4": {
            "MrCache\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MrCache\\Providers\\MrCacheServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
